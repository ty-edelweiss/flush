{
  "name": "Flush",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "34": "icon-34.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
      "34": "icon-34.png",
      "128": "icon-128.png"
  },
  "options_page": "options.html"
}
